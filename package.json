{
  "name": "meetyou-server",
  "version": "1.0.0",
  "description": "Server for Meetyou App",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "build": "nodemon ./bin/www",
    "test": "cross-env NODE_ENV=test npx jest --runInBand --detectOpenHandles --coverage --forceExit",
    "dev": "nodemon app.js",
    "db-dev": "sequelize-cli --env=test db:drop && sequelize-cli --env=test db:create && sequelize-cli --env=test db:migrate && sequelize-cli --env=test db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetyouapp/meetyou-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meetyouapp/meetyou-server/issues"
  },
  "homepage": "https://github.com/meetyouapp/meetyou-server#readme",
  "dependencies": {
    "axios": "^0.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jest": "^27.2.5",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.13",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
